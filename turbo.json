{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "clean": {
        "outputs": []
      },
      "test": {
        "dependsOn": ["build"],
        "outputs": [],
        "inputs": ["src/**/*.ts", "tst/**/*.ts"]
      }
    }
  }